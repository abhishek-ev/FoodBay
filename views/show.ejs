        <h6>Phone : <%= data.number %>  </h6>
        <h6>E-Mail : <%= data.email %>  </h6>
        <h6>Website : <a href="<%= data.website %>"><%= data.website %></a>  </h6>  <br><br>
        
        
        <!-- <button class="btn btn-outline-success book-seat" type="submit">Book Seat</button> -->
        <button type="button" class="btn btn-danger btn-outline-success book-seat" style="color: white ;" data-bs-toggle="modal" data-bs-target="#exampleModal" data-bs-whatever="@fat">Book Seat</button>
        <button class="btn-primary btn favourite-seat" style=" color: wheat;" type="submit"> FAV </button>
      </div>
      <div class="col-md-5">
        <div style="width:100%; height:100%;background-image: url('<%= data.image %>');"></div>

      </div>
    </div>


    <hr class="featurette-divider">


    <div class="row featurette">
      <div class="col-md-7">
        <h2 class="featurette-heading fw-normal lh-1"><%= data.name %> <span class="text-muted"> <br></span></h2>
        <p class="lead"><%= data.description %> </p>
        
        
        <br>

        <h6>Seats : <%= data.seats %></h6>
